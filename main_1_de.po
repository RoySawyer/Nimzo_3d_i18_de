msgid "Tournament terminated"
msgstr "Turnier beendet"

#: main_1.cpp:39851 main_1.cpp:39860
msgid "Display schedule"
msgstr "Zeitplan anzeigen"

#: main_1.cpp:39851 main_1.cpp:39860 main_1.cpp:44158 main_1.cpp:66529
#: main_1.cpp:133053
msgid "Close"
msgstr "Schließen"

#: main_1.cpp:39860
msgid "Display cross table"
msgstr "Kreuztabelle anzeigen"

#: main_1.cpp:39940
msgid "Cannot load engine 1 - Tournament game terminated"
msgstr "Engine 1 kann nicht geladen werden - Turnierspiel beendet"

#: main_1.cpp:39954
msgid "Cannot load engine 2 - Tournament game terminated"
msgstr "Engine 2 kann nicht geladen werden - Turnierspiel beendet"

#: main_1.cpp:40863
msgid "Tournament file name"
msgstr "Turnierdateiname"

#: main_1.cpp:40876
msgid "Tournament type -"
msgstr "Turniertyp -"

#: main_1.cpp:40882 main_1.cpp:42507
msgid "Single round robin"
msgstr "Einzelnes Rundenturnier"

#: main_1.cpp:40888 main_1.cpp:42517
msgid "Double round robin"
msgstr "Doppeltes Rundenturnier"

#: main_1.cpp:40894 main_1.cpp:42527
msgid "Swiss system"
msgstr "Schweizer System"

#: main_1.cpp:40900
msgid "Knockout seeded"
msgstr "Im Knockout gesetzt"

#: main_1.cpp:40906
msgid "Knockout random"
msgstr "Knockout zufällig"

#: main_1.cpp:40912
msgid "Number of rounds in the tournament"
msgstr "Anzahl der Runden im Turnier"

#: main_1.cpp:40918
msgid "Number of games in current round"
msgstr "Anzahl der Spiele in der aktuellen Runde"

#: main_1.cpp:40936
msgid "Round number"
